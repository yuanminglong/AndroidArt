<manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="com.yuan.androidart">
<!-- To access Google+ APIs: -->
<uses-permission android:name="android.permission.INTERNET" />

<application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/AppTheme"
    android:networkSecurityConfig="@xml/network_security_config">

    <activity
        android:theme="@style/Theme.AppCompat"
        android:name=".ui.NavigationActivity"
        android:label="@string/title_activity_navigation">
        <intent-filter>
            <action android:name="android.intent.action.VIEW" />

            <category android:name="android.intent.category.DEFAULT" />
            <category android:name="android.intent.category.BROWSABLE" />

            <data
                android:scheme="http"
                android:host="www.yuan.com">

            </data>
        </intent-filter>
        <nav-graph android:value="@navigation/nav_navigation" />
    </activity>
    <activity android:name=".ui.DataBindingActivity" />
    <activity android:name=".ui.ViewBindingActivity" />
    <activity
        android:exported="true"
        android:name=".ui.FragmentActivity"
        android:label="@string/title_activity_fregment"
        android:theme="@style/AppTheme.NoActionBar" />
    <activity
        android:name=".ui.MainActivity"
        android:theme="@style/AppTheme.NoActionBar">
        <!--        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>-->
    </activity>
    <activity android:name=".ui.RooMActivity" />
    <activity android:name=".ui.CustomViewActivity" />
    <activity android:name="com.yuan.androidart.ui.OkhttpActivity"></activity>
    <activity android:name="com.yuan.androidart.ui.RxJavaActivity">

    </activity>
    <activity android:name="com.yuan.androidart.ui.startmode.StandMode"
        android:launchMode="standard">
    </activity>
    <activity android:name="com.yuan.androidart.ui.startmode.SingleInstanceMode"
        android:launchMode="singleInstance"></activity>
    <activity android:name="com.yuan.androidart.ui.startmode.SingleTopMode"
        android:launchMode="singleTop"></activity>
    <activity android:name="com.yuan.androidart.ui.startmode.SingleTaskMode"
        android:launchMode="singleTask"></activity>
    <activity android:name="com.yuan.androidart.inflator.InflatorTestActivity" android:label="Inflate">

    </activity>
    <activity android:name="com.yuan.androidart.permission.PermissionTestActivity" android:label="Inflate">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />
            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
</application>

</manifest>


